{
    "name": "react-compat-ruler",
    "version": "0.2.0",
    "description": "A React Compat Ruler component that can draw grids and scroll infinitely.",
    "main": "./dist/ruler.cjs.js",
    "module": "./dist/ruler.esm.js",
    "sideEffects": false,
    "types": "declaration/index.d.ts",
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "start": "open ./demo/index.html && rollup -c rollup.config.demo.js -w",
        "build": "rollup -c && npm run declaration && print-sizes ./dist ",
        "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json"
    },
    "keywords": [
        "scene",
        "scenejs",
        "scene.js",
        "scena",
        "ruler",
        "scroller",
        "react",
        "compat"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/daybrush/ruler/blob/master/packages/react-compat-ruler"
    },
    "author": "Daybrush",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/daybrush/ruler/issues"
    },
    "homepage": "https://daybrush.com/ruler",
    "devDependencies": {
        "@daybrush/builder": "^0.1.0",
        "@scenejs/effects": "^1.0.1",
        "keycon": "^0.3.0",
        "preact": "^10.0.0",
        "print-sizes": "^0.1.0",
        "rollup-plugin-css-bundle": "^1.0.4",
        "rollup-plugin-preact": "^0.5.2",
        "rollup-plugin-react-compat": "^0.1.0",
        "scenejs": "^1.1.4",
        "shape-svg": "^0.3.3",
        "tslint": "^5.16.0",
        "typescript": "^3.4.5"
    },
    "dependencies": {
        "@scena/react-ruler": "^0.3.0",
        "@types/react": "^16.9.17",
        "react-simple-compat": "^0.1.4"
    },
    "peerDependencies": {
        "react-simple-compat": ">=0.1.0"
    }
}
