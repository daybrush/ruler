{
    "name": "@scena/vue-ruler",
    "version": "0.16.1",
    "description": "A Vue Ruler component that can draw grids and scroll infinitely.",
    "main": "./dist/ruler.cjs.js",
    "module": "./dist/ruler.esm.js",
    "types": "declaration/Ruler.d.ts",
    "sideEffects": false,
    "keywords": [
        "scene",
        "scenejs",
        "scene.js",
        "scena",
        "ruler",
        "scroller",
        "vue"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/daybrush/ruler.git"
    },
    "author": "Daybrush",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/daybrush/ruler/issues"
    },
    "homepage": "https://github.com/daybrush/ruler#readme",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "rollup -c && npm run declaration",
        "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json && cp ./src/components/Ruler.vue.d.ts ./declaration/"
    },
    "dependencies": {
        "@scena/ruler": "~0.18.1"
    },
    "files": [
        "README.md",
        "package.json",
        "dist/*",
        "src/*",
        "declaration/*"
    ],
    "devDependencies": {
        "@daybrush/builder": "^0.1.2",
        "@vue/cli-plugin-typescript": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^2.7.0",
        "@vue/composition-api": "^1.2.4",
        "@vue/eslint-config-airbnb": "^5.0.2",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^5.0.2",
        "@vue/test-utils": "^1.0.3",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "rollup-plugin-vue": "^5.1.9",
        "typescript": "^4.5.0 <4.6.0",
        "vue": "^2.6.12",
        "vue-class-component": "^7.2.6",
        "vue-property-decorator": "^9.1.2",
        "vue-template-compiler": "^2.6.12"
    }
}
