{
    "plugins": [],
    "recurseDepth": 10,
    "opts": {
        "template": "./node_modules/daybrush-jsdoc-template",
        "destination": "./doc/"
    },
    "source": {
        "include": [
            "README.md",
            "packages/ruler/src",
            "packages/react-ruler/src/react-ruler"
        ],
        "includePattern": "(.+\\.js(doc|x)?|.+\\.ts(doc|x)?)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "linkMap": {
        "IObject": "http://daybrush.com/utils/release/latest/doc/global.html#ObjectInterface"
    },
    "docdash": {
        "menu": {
            "Github repo": {
                "href": "https://github.com/daybrush/ruler",
                "target": "_blank",
                "class": "menu-item",
                "id": "repository"
            }
        }
    }
}
